{
    "presets": [
        [ "env", {
            "targets": {
                "node": 6.0
            }
        } ],
        "es2016",
        "es2017",
        "stage-3",
        "stage-2"
    ],
    "plugins": [
        [ "transform-runtime", {
            "helpers":     true,
            "polyfill":    true,
            "regenerator": false,
            "moduleName": "babel-runtime"
        } ]
    ]
}
