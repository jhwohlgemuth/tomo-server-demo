{
    "name": "tomo-server",
    "version": "0.0.0",
    "description": "Node.js HTTP(S), WebSocket, and GraphQL servers with an 80% solution for security 'baked in'",
    "license": "MIT",
    "keywords": [],
    "scripts": {
        "lint": "eslint . -c ./.eslintrc.js --fix",
        "lint:ing": "watch 'npm run lint' .",
        "lint:tests": "eslint __tests__/**/*.js -c ./.eslintrc.js --fix --no-ignore",
        "test": "jest .*.test.js --coverage",
        "test:ing": "npm test -- --watchAll",
        "predev": "npm run open",
        "dev": "stmux [ \"nodemon index.js\" : \"npm run lint:ing\" ]",
        "prestart": "npm audit --production",
        "start": "node index.js",
        "open": "open-cli http://localhost:8111"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.2",
        "babel-jest": "^24.8.0",
        "eslint": "^6.1.0",
        "eslint-config-omaha-prime-grade": "^11.0.4",
        "jest": "^24.8.0",
        "jest-watch-typeahead": "^0.3.1",
        "nodemon": "^1.19.1",
        "open-cli": "^5.0.0",
        "stmux": "^1.7.1",
        "supertest": "^4.0.2",
        "watch": "^1.0.2"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.(e2e|test).[jt]s?(x)"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "main": "index.js",
    "dependencies": {
        "apollo-server-express": "^2.8.1",
        "compression": "^1.7.4",
        "config": "^3.2.2",
        "cookie-session": "^1.3.3",
        "dotenv": "^8.0.0",
        "ejs": "^2.6.2",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "fs-extra": "^8.1.0",
        "graphql": "^14.4.2",
        "helmet": "^3.20.0",
        "highlight.js": "^9.15.9",
        "lusca": "^1.6.1",
        "npmlog": "^4.1.2",
        "protocolify": "^3.0.0",
        "remarkable": "^1.7.4",
        "uuid": "^3.3.2",
        "ws": "^7.1.1"
    }
}
